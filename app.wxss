page,.container{
  width: 100%;
  height: 100%;
  background: #f6f7f9;
}

.textColor {
  color: transparent;
  background-clip: text;
  -webkit-background-clip: text;
  background-image: radial-gradient(circle, #39b54a, #a8e063);
}

.flexCenter {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}

.bgcolor{
  background-color: #39b54a;
  /* background: linear-gradient(to bottom,0 #FE5858,70% #FF416C, #FF4B2B, #FE5858); */

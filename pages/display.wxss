.icon-location {
  padding-right: 5rpx;
}

.location {
  max-width: 126rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  transition: all 0.1s;
  padding: 16rpx 0;
}

.search-round {
  background: rgba(255, 255, 255, 0.9) !important;
  /* transition: all 1s; */
}

.search-input {
  flex: 1;
  padding-right: 32rpx;
}

.search-board {
  height: 0;
  opacity: 0;
  background: rgba(225, 225, 225, 0.7);
  transition: all 0.3s;
  overflow: auto;
}

.list-li {
  border-bottom: none !important;
}

.history-item {
  width: auto;
  height: auto;
  padding: 10rpx;
  max-width: 250rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  margin: 5rpx 10rpx;
  background: rgba(238, 238, 238, 0.7);
  border-radius: 8rpx;
}

.suggestion-item {
  width: 100%;
  height: auto;
  padding: 0rpx 30rpx;
  margin-bottom: 36rpx;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  display: flex;
  align-items: flex-start;
  border: none;
  position: relative;
  font-size: 32rpx;
}

.search-suggestion-text {
  padding-left: 15rpx;
}

.search-suggestion-icon {
  width: auto !important;
  line-height: inherit !important;
  font-size: inherit !important;
}

/* 系统列表 */

.goods-list-li {
  margin-bottom: initial;
  border-bottom: 0.5rpx solid #eee;
}

/* 系统头像部分 */

.s-avatar-wrap {
  width: 160rpx;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  justify-content: space-between;
  align-items: flex-start;
  margin-top: 8rpx;
}

.s-avatar {
  width: 160rpx;
  height: 160rpx;
}

/* 系统信息部分 */

.icon-image {
  background: inherit !important;
  border-radius: 0 !important;
}

.rating-item {
  display: flex;
  line-height: 60rpx;
}

.rating-text.gray-text text {
  padding: 0 10rpx;
}

.s-name {
  flex: 1;
  padding-right: 10rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 260rpx;
}

.s-address {
  color: #dd514c;
  /* font-size: 36rpx; */
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  max-width: 480rpx;
}

.s-header {
  display: flex;
  align-items: center;
  justify-content: space-between;
  font-weight: bold;
}

.s-list-top {
  /* border-bottom: .5rpx solid rgba(238, 238, 238, 0.7); */
  width: 100%;
  height: 160rpx;
}

/* 系统备注 */

.s-tips {
  max-width: 360rpx;
}

.s-tips text {
  padding: 5rpx 10rpx;
}

.s-tips text:last-child {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  display: initial;
}

.s-time {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  border: none;
}

.s-time text {
  padding: 1rpx 5rpx;
  border-bottom: 0.5rpx solid #0081ff;
  font-size: 24rpx;
}

.contentIsNull {
  /* height: 680rpx; */
  height: 520rpx;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  font-size: 48rpx;
}

.contentIsNull image {
  height: 256rpx;
  width: 256rpx;
  padding: 30rpx;
}

/* table */

.table {
  /* border-top: 8rpx solid darkgray; */
  padding: 0 1rem 1rem;
  border-radius: 8rpx;
}

.ttitle {
  display: flex;
  /* align-items: center; */
  font-weight: bold;
  font-size: 48rpx;
}

.table.t-green .thead {
  background: rgba(57, 181, 74, 0.8);
}

.table.t-blue .thead {
  background: #36f;
}

.thead, .tr {
  display: flex;
  width: 100%;
  justify-content: center;
  height: 3rem;
  align-items: center;
}

.td {
  display: flex;
  width: 40%;
  justify-content: center;
  text-align: center;
}

.th {
  width: 40%;
  justify-content: center;
  color: #fff;
  display: flex;
  height: 3rem;
  align-items: center;
  font-weight: 24rpx;
}

.tt {
  width: 100%;
  height: auto;
  padding: 8rpx;
  font-weight: 24rpx;
  background: darkgray;
}

.t-bg-w {
  background: snow;
}

.table.table.t-green .t-bg-g {
  background: #deeede;
}

.table.t-blue .t-bg-g {
  background: #e6f3f9;
}

/* icon nav */

.icon-nav-box {
  overflow: hidden;
}

.icon-nav-box .icon-nav {
  height: auto;
  width: 100%;
  padding: 30rpx 0 0;
  align-items: center;
}

.icon-nav-box .icon-nav-navigator.active {
  border-bottom: 5rpx solid #f37b1d;
  border-radius: 3rpx;
}
